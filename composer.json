    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Tjmpromos\\SortableGallery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tjmpromos\\SortableGallery\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tjmpromos\\SortableGallery\\SortableGalleryServiceProvider"
            ],
            "aliases": {
                "SortableGallery": "Tjmpromos\\SortableGallery\\Facades\\SortableGallery"
            }
        }
    }
