{
    "name": "tjmpromos/sortable-gallery",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Author Name",
            "email": "author@email.com",
            "homepage": "http://author.com"
        }
    ],
    "homepage": "https://github.com/tjmpromos/sortable-gallery",
    "keywords": ["Laravel", "SortableGallery"],
    "require": {
